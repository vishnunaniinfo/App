{
  "name": "xavira-lead-engine-backend",
  "version": "1.0.0",
  "description": "Backend API for Xavira Lead Engine - Real estate lead management SaaS with WhatsApp automation",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset"
  },
  "keywords": ["real-estate", "crm", "lead-management", "whatsapp", "automation", "saas"],
  "author": "Xavira Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1",
    "prisma": "^5.7.1",
    "@prisma/client": "^5.7.1",
    "bullmq": "^4.15.0",
    "ioredis": "^5.3.2",
    "nodemailer": "^6.9.7",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.1",
    "twilio": "^4.19.0",
    "axios": "^1.6.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "openapi-types": "^12.1.3",
    "cookie-parser": "^1.4.6",
    "express-async-errors": "^3.1.1"
